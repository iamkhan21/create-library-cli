{
  "module": {
    "type": "commonjs",
    "strict": true
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "target": "es2016",
    "minify": {
      "compress": true,
      "mangle": true
    }
  },
  "minify": true,
  "exclude": "^.+\\.test.ts$"
}
