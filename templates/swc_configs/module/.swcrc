{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "target": "es2020",
    "minify": {
      "compress": {
        "dead_code": true,
        "unused": true
      },
      "mangle": true
    }
  },
  "minify": true,
  "exclude": "^.+\\.test.ts$"
}
