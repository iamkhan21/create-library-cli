{
  "name": "create-library-cli",
  "version": "2.0.0",
  "description": "The interactive CLI will help you create and configure your library project automatically.",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/iamkhan21/create-library-cli.git"
  },
  "license": "Apache-2.0",
  "author": "Ivan Khanevich <ivan.khanevich@gmail.com>",
  "type": "module",
  "exports": "./bin/index.js",
  "bin": {
    "create-library-cli": "bin/index.js"
  },
  "files": [
    "bin/",
    "templates/",
    "LICENSE",
    "README.md",
    "codecov.yml",
    ".prettierignore",
    ".nvmrc",
    ".editorconfig"
  ],
  "scripts": {
    "create-library-cli": "node bin/index.js",
    "semantic-release": "semantic-release",
    "update:deps": "npx npm-check-updates -i && npm i"
  },
  "dependencies": {
    "inquirer": "^8.1.5",
    "mico-spinner": "^1.3.0",
    "nanocolors": "^0.2.12"
  },
  "devDependencies": {
    "eslint": "8.12.0",
    "prettier": "2.4.1",
    "prettier-plugin-packagejson": "2.2.17",
    "semantic-release": "18.0.1",
    "semantic-release-cli": "5.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "renovate": {
    "extends": [
      "config:js-lib",
      ":automergePatch",
      ":automergeBranch",
      ":automergePatch",
      ":automergeBranch",
      ":automergeLinters",
      ":automergeTesters",
      ":automergeTypes"
    ]
  }
}
